<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
	<rule>
        <from>/app/css/(.*)$</from>
        <to>/dist/css/$1</to>
    </rule>
    <rule>
        <from>/app/components/(.*)$</from>
        <to>/dist/js/components/$1</to>
    </rule>
    <rule>
        <from>/app/src/(.*)$</from>
        <to>/dist/js/src/$1</to>
    </rule>
    <rule>
        <from>/app/js/(.*)$</from>
        <to>/dist/js/$1</to>
    </rule>
	<rule>
		<from>/app/(.*)$</from>
		<to>/dist/index.html</to>
	</rule>
	<rule>
		<from>^(/)?$</from>
		<to type="redirect">/app/home</to>
	</rule>
</urlrewrite>